function (a1 a2 N) {
#this function takes as input a1 a2 N and returns sum and fiboN#
    if (N==1) {
        fibo = a1
        sum = a1
    }
    else {
        if (N==2) {
            fibo = a2
            sum = a1 + a2
        }
        else {
        i = 3
        sum = a1 + a2
        anow = a2
        aprev = a1
            while (i<=N) {
                temp = anow
                anow = anow+aprev
                aprev = temp
                i = i+1
                sum = sum + anow
            }
            fibo = anow
        }
    }

return (fibo sum)
}

        
